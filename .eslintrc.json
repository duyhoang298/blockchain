{
    "extends": ["airbnb-base","prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "parser": "babel-eslint",
    "rules": {
      "indent": [2, 2, { "SwitchCase": 1 }],
      "no-console": 0,
      "object-curly-spacing": [2, "always"],
      "no-param-reassign": 0,
      "no-plusplus":0,
      "func-names": 0,
      "dot-notation":1,
      "prefer-template": 0
    }
  }
  